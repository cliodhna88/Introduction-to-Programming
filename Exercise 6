password = 'python123'
max_attempts = 5

for attempt in range(1, max_attempts + 1):
    user_input = input("Please enter Password: ")

    if user_input == password:
        print("!CORRECT!")
        break
    else:
        if attempt < max_attempts:
            print("!WRONG! Try Again")
        else:
            print("!ACCESS DENIED! Maximum attempts reached")
